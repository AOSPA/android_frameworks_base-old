<resources>

    <!-- Color of the UDFPS pressed view -->
    <color name="config_udfpsColor">#ffffffff</color>

    <!-- Whether usage of the proximity sensor during doze is supported -->
    <bool name="doze_proximity_sensor_supported">true</bool>

    <!-- Doze: does the double tap sensor need a proximity check? -->
    <bool name="doze_double_tap_proximity_check">false</bool>

    <!-- Doze: does the pickup sensor need a proximity check? -->
    <bool name="doze_pickup_event_proximity_check">false</bool>

    <!-- Doze: does the single tap sensor need a proximity check? -->
    <bool name="doze_single_tap_proximity_check">false</bool>

    <!-- Whether long press is required to activate keyguard quick affordance buttons -->
    <bool name="config_keyguardQuickAffordanceRequiresLongPress">true</bool>

    <!-- Whether to enable burn-in protection for statusbar and navbar -->
    <bool name="config_statusBarBurnInProtection">@*android:bool/config_enableBurnInProtection</bool>

    <!-- Interval between subsequent shifting of statusbar and navbar contents, in seconds -->
    <integer name="config_statusBarBurnInProtectionShiftInterval">60</integer>

    <!-- Allow devices override audio panel location to the left side -->
    <bool name="config_audioPanelOnLeftSide">false</bool>

</resources>
