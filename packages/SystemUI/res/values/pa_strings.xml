    <string name="zen_footer_alert_slider_priority_only_summary">Specified notifications only</string>
    <string name="zen_footer_alert_slider_alarms_only_summary">Alarms and media playback only</string>
    <string name="zen_footer_alert_slider_no_interruptions_summary">No sounds and interruptions</string>

    <string name="quick_settings_alert_slider_detail_priority_events_title">Events</string>
    <string name="quick_settings_alert_slider_detail_priority_reminders_title">Reminders</string>
