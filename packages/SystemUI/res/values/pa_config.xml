<resources>

    <!-- Allow devices override audio panel location to the left side -->
    <bool name="config_audioPanelOnLeftSide">false</bool>

    <!-- Max visible notification icons -->
    <integer name="config_maxVisibleNotificationIcons">4</integer>
    <integer name="config_maxVisibleNotificationIconsOnLock">5</integer>

    <!-- Color of the FOD view -->
    <color name="config_fodColor">#00ff00</color>
    <color name="config_fodColorBackground">#20000000</color>

    <!-- Whether to dim the FOD icon -->
    <bool name="config_fodIconDim">false</bool>

    <!-- Allow devices to use unordinary cutout overlays -->
    <bool name="config_customCutout">false</bool>

    <!-- Allow devices to hide black gradient behind QS Panel -->
    <bool name="config_hideQSBlackGradient">false</bool>

    <!-- Additional default tiles to display in QuickSettings -->
    <string name="quick_settings_tiles_extra_default" translatable="false">
    </string>

    <!-- Additional QS Tiles. Order should match "quick_settings_tiles_extra_default" -->
    <string name="quick_settings_tiles_extra" translatable="false">
        caffeine,screenstabilization,screen_record,dataswitch,heads_up
    </string>

    <!-- Allow devices to use a lower screenrecording bitrate because of old hardware -->
    <bool name="config_device_needs_lower_bitrate">false</bool>

</resources>
